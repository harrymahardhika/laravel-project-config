{
    "preset": "laravel",
    "rules": {
        "class_attributes_separation": {
            "elements": {
                "const": "only_if_meta"
            }
        },
        "no_superfluous_phpdoc_tags": true,
        "phpdoc_align": {
            "align": "left"
        },
        "yoda_style": true
    }
}
